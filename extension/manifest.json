{
  "manifest_version": 3,
  "name": "Valor2FA",
  "version": "1.0.0",
  "description": "A secure & open source 2FA code generator by Valortraders",
  "icons": {
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  }
} 